div.autocomplete {
  position:absolute;
  width:250px;
  background-color:white;
  border:1px solid #888;
  margin:0px;
  padding:0px;
}

div.autocomplete ul {
  list-style-type:none;
  margin:0px;
  padding:0px;
}
div.autocomplete ul li.selected { background-color: #ffb;}
div.autocomplete ul li {
  list-style-type:none;
  display:block;
  margin:0;
  padding:2px;
  height:32px;
  cursor:pointer;
}

.item {
	padding: 0.2em;
	width: 95%;
	line-height: 18px;
	margin-bottom: 0.4em;
}

.item name {

}

.item .window {
	color: black !important;
	display:inline;
	margin-left:50em;
	padding:0.7em;
	position:absolute;
	width:20em;
	margin-top:-7px;
}

.item span
{
	display: inline-block;
	overflow: hidden;
}

.item .window div {
	margin-bottom: 0.4em;
}

.hidden {
