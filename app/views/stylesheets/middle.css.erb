h4{
  font-size: 110%;
  margin: 10px 0 10px 0;
  padding: 0;
}

.notice {
	padding: 10px 10px 10px 30px;
	margin-bottom: 1em;
	border: 2px solid #1D9305;
	font-size: 90%;
	font-weight: bold;
	color:#666;
	line-height: 1;
	background: #3AC21F url(/images/accept.png) left no-repeat;
	background-position: 10px 7px;
}

.error {
	padding: 10px 10px 10px 30px;
	margin-bottom: 1em;
	border: 2px solid #ff3333;
	font-size: 90%;
	font-weight: bold;
	color: #d30000;
	line-height: 1;
	background: #ff9999 url(/images/cancel.png) left no-repeat;
	background-position: 10px 7px;
}

.CommonMessage{
	clear:both;
	margin:5px 0;
	padding-left:190px;
	padding-top:10px;
	width:460px;
}
.red{
  color: red;
}

img.test{
  clear: left;
  float:left;
} 

#middle-middle{
  margin-right: 180px;
  padding: 20px 20px 40px 20px;
  font-size: 95%;
  width: 760px;
  overflow: hidden;
}

#middle-right{
  float: right;
  width: 180px;
  margin-top: 10px;
}

#middle-right select{
  width: 170px;
}

#middle-right h3{
  margin-top: 10px;
  padding-bottom: 4px;
  font-size: 97%;
  color: #F79646;
  border-bottom: 1px #F2F2F2 solid;
}

#midle-right-menu{
	text-align:right;
	padding-right:20px;
}

#midle-right-menu p{
	margin:0;
}
#search-bar-container{
  width: 780px;
  margin: 0px 0px 20px 0px;
}

#search-bar-left, #search-bar-right, #search-bar-middle{
  float: left;
  height: 67px;
}

#search-bar-left, #search-bar-right{
  width: 10px;
}

#search-bar-left{
   background:#AB5D7C url(/images/search_bg_left.png) no-repeat scroll 0 0;
}

#search-bar-right{
  background:#AB5D7C url(/images/search_bg_right.png) no-repeat scroll 0 0;
}

#search-bar-middle{
  background:#AB5D7C url(/images/search_bg_middle.png) repeat-x scroll 0 0;
  color: #fff;
  text-align: right;
  width:740px;
}

a.advanced_search, a:hover.advanced_search, a:link.advanced_search, a:active.advanced_search, a:visited.advanced_search{
  font-size: 75%;
  color: #fff;
}

a.search_button{
  float: left;
  margin: 13px 12px 0px 17px;
}

#search-bar-middle form{
  margin: 0;
  padding: 0px;
}

#search-bar-middle input.style_text, #search-bar-middle select{
  float: left;
  width: 310px;
  margin-left: 10px;
  margin-top: 13px;
  font-size: 140%;
}

#search-bar-middle select{
  height:31px;
}

#search-bar-middle p{
  margin: 15px 0 0 0;
  padding: 0;
  display: block;
  float: left;
  margin-left: 10px;
  font-size: 140%;
}

img.avatar{
}

#area_TITLE{
	margin: 20px 0 20px 0;
	border-bottom: 1px solid #f2f2f2;
	padding-bottom:10px;
}

.area_LEFT{
  float: left;
  width: 370px;
}

.area_RIGHT{
  float: right;
  width: 370px;
}

.area_BOTH{
  margin-top: 20px;
}

.area_LEFT ul{
	padding-left:10px;
	list-style-type:none;
}

.area_LEFT ul li{
	margin-top:10px;
}

#area_INFOS{
}

#area_INFOS p{
  margin: 0px 0px 5px 0px;
  padding: 0;
}

#area_INFOS p.name{
  font-size: 110%;
}

.area_WS{
  background-color:<%=@ws.bgcolor%>;
  border: 2px solid <%=@border.bgcolor%>;
  padding: 10px;
  margin-top:10px;
  margin-bottom: 10px;
}

.area_WS h4{
  color: #fff;
}

.area_WS fieldset{
  border: none;
  background: transparent;
  margin: 0;
  padding: 0;
}

.area_WS fieldset label{
  color: #fff;
}

.area_WS a{
	color:#ffffff;
}

.pubmed{
  padding-left: 100px;
}

.lift_items{
  padding-left: 10px;
  margin-top: 5px;
}

.label-txt{
  color: #000;
}

.content-txt{
  color: #444;
}

a.object{
  color: black;
}

.item-list{
  margin-top: 1em;
  color: #555;
  overflow:hidden;
}

.top_elem{
  border-bottom: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  padding: 10px;
}

a.blok{
  margin-top: 1px;
  background-color:#dbdbdb;
  display: block;
  padding: 7px 10px;
}

a:hover.blok, a:link.blok, a:active.blok, a:visited.blok{
  color: #fff;
  text-decoration: none;
}

a.blok_selected{
  background-color: #bdd6d9;
  color: #444d4e;
}

ul#tabs, ul#top_elems{
  margin: 0;
  padding: 0 0 29px 0;
  border-bottom: 1px solid #ccc;
}

a.active{
	background:#f3f3f3;
}

ul#tabs li, ul#top_elems li{
  float: left;
  list-style-type: none;
  margin-right: 3px;
  border-top: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}

ul#tabs li.selected, ul#top_elems li.selected{
  background: #f3f3f3;
  border-bottom: 1px solid #f1f1f1;
}

ul#tabs li.selected a, ul#top_elems li.selected a{
  color: #888;
}

ul.without_img li a{
  padding: 7px 7px !important;
}

ul#tabs li a, ul#top_elems li a{
  text-decoration: none;
  display: block;
  padding: 5px 8px;
  font-weight: bold;
  font-size: 95%;
}

ul#tabs li a img{
  margin-right: 3px;
}

ul.top_box{
  list-style-type: none;
  margin: 0;
  padding: 0;
}

ul.top_box li{
  margin-top: 10px;
}

.top_box a{
	color:#F79646;
}

.pubmed_item {
	background-color: #F4F4F4;
	padding: 10px;
	margin-top: 20px;
}

#object-list{
  background: #fff url(/images/top_elems_bg.png) repeat-x;
  padding: 20px;
  border-bottom: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}

#top_content{
  background: #fff url(/images/top_elems_bg.png) repeat-x;
  padding: 10px;
  border-bottom: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  overflow: hidden;
}

.file_infos_box {
	border:1px solid #cccccc;
	padding:5px 10px;
	margin:0px;
}

#page #container h1 {
	color:#F79646;
	font-size:175%;
	padding-bottom:7px;
	clear:both;
}

#content_show {
	margin:0 auto;
	width:760px;
	color:#666666;
}

#content_show h1 {
	margin:0px 0px 1px;
	padding:0px;
	border-bottom: 1px #F2F2F2 solid;
}

#content_show .undertitle {
	margin:2px 4px 0px 0px;
	padding:0px;
	float:left;	
	color:#000;
}

#content_show .tags {
	margin:5px 0px;
	clear:left;
	color:#000;
}

#content_show .file_infos_box {
	padding:5px 10px;
	margin-bottom:20px;
}

#content_show .file_infos_box img {
	position:relative;
	top:2px;
	margin: 0px 2px 0px 0px;
	padding:0px;
}

#content_show .file_infos_box a {
	margin:0px 25px 0px 0px;
	padding:0px;
}

#content_show h2 {
	font-size:110% !important;
	margin-bottom:15px;
	margin-top:25px;
	color:#000000;
}

#content_show fieldset#new_comment_form_box {
	padding:0px 30px 20px;
	margin-top:20px;
	margin-left:0px;
}

#content_show #new_comment_form_box textarea {
	width:100%;
	margin:0px 0px 20px;
}

#content_show #new_comment_form_box .buttons button.alone {
	margin-right:0;
	float:right;
}

#content_show #comment_list {
	margin-top:40px;
	padding:0px;
}

#content_show #comment_list img {
	float:left;
	position:relative;
	margin-right:10px;
}

#content_show h2#comments {
	font-size:100%;
	margin:0px 0px 0px 0px;
	padding:0px;
}

#content_show #comment_list table {
	font-size:100%;
	clear:left;
	margin:0px;
	border-top:1px solid black;
}

#content_show #comment_list table tr {
}

.display_search_result{
	border: 1px solid #F2F2F2;
	padding: 0.5em;
}
.display_search_result span{
	color:grey;
}

.pagination{
	text-align:center;
}

.other_output_format{
	float:right;
}