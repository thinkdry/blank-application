h4{
  font-size: 110%;
  margin: 10px 0 10px 0;
  padding: 0;
}

.notice {
	background:transparent url(/images/icons/accept.png) no-repeat scroll 7px 50%;
	color:#3AC21F;
	margin-bottom:2em;
	padding:20px 10px 20px 50px;
}

.error {
	padding:20px 10px 20px 50px;
	margin-bottom: 2em;
	color: #ff9999;
	background: transparent url(/images/icons/error.png) no-repeat scroll 7px 50%;
}

.CommonMessage{
	clear:both;
	margin:5px 0 5px 190px;
	padding-left:40px;
	padding-top:20px;
	padding-bottom:18px;
	width:460px;
	background: transparent url(/images/icons/info.png) no-repeat scroll 0px 50%;
}
.red{
  color: red;
}

img.test{
  clear: left;
  float:left;
} 

#middle-middle{
  margin-right: 180px;
  padding: 20px 20px 40px 20px;
  font-size: 95%;
  width: 760px;
  overflow: hidden;
}

#middle-right{
  float: right;
  width: 180px;
  margin-top: 10px;
}

#middle-right select{
  width: 170px;
}

#middle-right h3{
  margin-top: 10px;
  padding-bottom: 4px;
  font-size: 97%;
  color: #F79646;
  border-bottom: 1px #F2F2F2 solid;
}

#middle-right p{
	margin-right:5px;
}

#midle-right-menu{
	text-align:right;
	padding-right:20px;
}

#midle-right-menu p{
	margin:0;
	overflow:visible;
}
#midle-right-menu p img{
	margin:5px 5px 0 0;
}
#midle-right-menu a{
	margin-bottom:5px;
}
#search-bar-container{
  width: 780px;
  margin: 0px 0px 40px 0px;
}

#search-bar-left, #search-bar-right, #search-bar-middle{
  float: left;
  height: 67px;
}

#search-bar-left, #search-bar-right{
  width: 10px;
}

#search-bar-left{
   background:#AB5D7C url(/images/search_bg_left.png) no-repeat scroll 0 0;
}

#search-bar-right{
  background:#AB5D7C url(/images/search_bg_right.png) no-repeat scroll 0 0;
}

#search-bar-middle{
  background:#AB5D7C url(/images/search_bg_middle.png) repeat-x scroll 0 0;
  color: #fff;
  text-align: right;
  width:740px;
}

a.advanced_search, a:hover.advanced_search, a:link.advanced_search, a:active.advanced_search, a:visited.advanced_search{
  font-size: 75%;
  color: #fff;
}

a.search_button{
  float: left;
  margin: 13px 12px 0px 17px;
}

#search-bar-middle form{
  margin: 0;
  padding: 0px;
}

#search-bar-middle input.style_text, #search-bar-middle select{
  float: left;
  width: 310px;
  margin-left: 10px;
  margin-top: 13px;
  font-size: 140%;
  color:#666666;
}

#search-bar-middle select{
  height:31px;
}

#search-bar-middle p{
  margin: 15px 0 0 0;
  padding: 0;
  display: block;
  float: left;
  margin-left: 10px;
  font-size: 140%;
}

#area_TITLE{
	margin: 20px 0 20px 0;
	border-bottom: 1px solid #f2f2f2;
	padding-bottom:10px;
}

.area_LEFT{
  float: left;
  width: 370px;
  margin-bottom:20px;
}

.area_RIGHT{
  float: right;
  width: 370px;
  margin-bottom:20px;
}

.area_BOTH{
  margin-top: 20px;
  clear:both;
}

.area_LEFT ul{
	padding-left:10px;
	list-style-type:none;
}

.area_LEFT ul li{
	margin-top:10px;
	min-height:35px;
}

#area_INFOS p{
  margin: 0px 0px 5px 0px;
  padding: 0;
}

.area_WS{
  background-color:<%=@ws.bgcolor%>;
  border: 2px solid <%=@border.bgcolor%>;
  padding: 10px;
  margin-top:10px;
  margin-bottom: 10px;
  color:#666666;
}

.area_WS h4{
  color: #fff;
}

.area_WS fieldset{
  border: none;
  background: transparent;
  margin: 0;
  padding: 0;
}

.area_WS fieldset label{
  color: #fff;
}

.area_WS a{
	color:#ffffff;
}

.pubmed{
  padding-left: 100px;
}

.lift_items{
  padding-left: 10px;
  margin-top: 5px;
}

.content-txt{
  color: #444;
}

a.object{
  color: black;
}

.item-list{
  color: #9C9C9C;
  overflow:hidden;
}

.top_elem{
  border-bottom: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  padding: 10px;
}

a.blok{
  margin-top: 1px;
  background-color:#dbdbdb;
  display: block;
  padding: 7px 10px;
}

a:hover.blok, a:link.blok, a:active.blok, a:visited.blok{
  color: #fff;
  text-decoration: none;
}

a.blok_selected{
  background-color: #bdd6d9;
  color: #444d4e;
}

ul#tabs, ul#top_elems{
  margin: 0;
  padding: 0 0 39px 0;
  border-bottom: 1px solid #ccc;
}

a.active{
	background:#f3f3f3;
}

ul#tabs li, ul#top_elems li{
  float: left;
  list-style-type: none;
  margin-right: 3px;
  border-top: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  height:39px;
}

ul#tabs li.selected, ul#top_elems li.selected{
  background: #f3f3f3;
  border-bottom: 1px solid #f1f1f1;
}

ul#tabs li.selected a, ul#top_elems li.selected a{
  color: #666;
}

ul.without_img li a{
  padding: 13px !important;
}

ul#tabs li a, ul#top_elems li a{
  text-decoration: none;
  display: block;
  padding: 2px 8px;
  font-size: 80%;
  color:#9C9C9C;
}

ul#tabs li a img{
  margin-right: 3px;
}

ul.top_box{
  list-style-type: none;
  margin: 0;
  padding: 0;
}

ul.top_box li{
  margin-top: 10px;
  min-height:35px;
}

.pubmed_item {
	background-color: #F4F4F4;
	padding: 10px;
	margin-top: 20px;
}

#object-list{
  background: #fff url(/images/top_elems_bg.png) repeat-x;
  padding:50px 20px 20px;
  border-bottom: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}

#top_content{
  background: #fff url(/images/top_elems_bg.png) repeat-x;
  padding: 10px;
  border-bottom: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  overflow: hidden;
}

#page_container #container h1 {
	padding-bottom:7px;
	clear:both;
}

#content_show {
	margin:0 auto;
	width:760px;
	color:#666666;
}

#content_show h1 {
	margin:0px 0px 1px;
	padding:0px;
	border-bottom: 1px #F2F2F2 solid;
}

#content_show .file_infos_box {
	float:left;
	margin:7px 0 0;
	width:253px;
}

#content_show .file_infos_box img {
	padding:0 3px;
}

#content_show h2 {
	margin-bottom:15px;
	margin-top:25px;
	clear: both;
}

#content_show fieldset#new_comment_form_box {
	padding:0px 30px 20px;
	margin-top:20px;
	margin-left:0px;
	clear:both;
}

#content_show #new_comment_form_box textarea {
	width:100%;
	margin:0px 0px 20px;
}

.display_search_result{
	border: 1px solid #F2F2F2;
	padding: 0.5em;
}
.display_search_result span{
	color:grey;
}

.pagination{
	text-align:center;
}

.other_output_format{
	float:right;
	color:#9C9C9C;
}

.file_infos_box p{
	margin:0;
}

.output_format{
	margin-top:14px;
	text-align:right;
}
.output_format_link{
	margin:0 !important;
}

.file_infos{
	padding-bottom:7px;
	border-bottom:1px solid #F2F2F2;
	overflow:auto;
}

.comment_info, .comment_text{
	float:left;
	margin-bottom:10px;
}
.comment_info{
	clear:both;
	margin-right:50px;
}
.comment_bloc{
	margin-top:10px;
	overflow:auto;
}
.comment_label{
	padding-top:5px;
}
.object_title{
	font-size:125%;
	color:#F79646 !important;
}
#object-list p{
	margin:0;
}

.item_list_icon{
	clear:both;
	float:left;
	margin-right:10px;
}
.item_list_body{
	float:left;
}

.itme_list_item_title_large{
	float:left;
	width:365px;
}
.item_list_item_infos{
	float:left;
	text-align:right;
	width:300px;
	line-height:20px;
}
.item_list_item_actions{
	float:left;
	width:32px;
	margin-left:10px;
}

.itme_list_item_title_small{
	float:left;
	width:335px;
}
.item_list_rate{
	float:right;
}
#attachement, #comment_list{
	overflow:auto;
	padding-top:20px;
}
#attachement h2, #comment_list h2{
	border-bottom:1px solid #F2F2F2;
	padding-bottom:7px;
}
#progress{
	margin-top:20px;
}

.tooltip_actions{float:left;width:35px;}
.tooltip_infos{float:left;width:250px;text-align:left;}
.tooltip_hidden{display:none;}

.item_description{
	font-size:115%;
	color:#aaa;
	margin:20px 0;
}

#files{
	margin-top:3px;
}

// copied from Lacriee
#left_select_box, #left_right_buttons, #right_select_box{
	float:left;
}

#left_select_box select, #right_select_box select{
	width:200px;
}
.optionAssignmentLabel{
	margin-bottom:3px;
}
.theatreAssignment{
	width:50px;
}
#left_right_buttons{
	width:50px;
	margin:20px 10px;
}
#select_boxes{
	margin-top:4px;
}
